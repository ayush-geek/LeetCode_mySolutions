class   Solution   { 
 public : 
     vector < int >   countBits ( int  n )   { 
         vector < int >  res ; 
         
       for ( int  i = 0 ; i <= n ; i ++ ) 
       { 
          int  ct = 0 ; 
          int  z = i ; 
          while ( z != 0 ) 
          {    
              if ( z % 2 != 0 ) 
                 ct ++ ; 
             z = z / 2 ; 
             
          } 
 
          res . push_back ( ct ) ; 
       }   
 
       return  res ;  
      } 
 } ;