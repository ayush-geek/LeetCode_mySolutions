class   Solution   { 
  public : 
      int   trailingZeroes ( int  n )   { 
         
          int  c = 0 ; 
          int  s = 5 ; 
 
          while ( s <= n ) 
          { 
             c += n / s ; 
             s *= 5 ; 
         
         
          } 
          return  c ; 
      }    
  } ;