class   Solution   { 
 public : 
      int   largestAltitude ( vector < int > &  gain )   { 
         
          int  ini = 0 ; 
          int  mx = 0 ; 
 
          for ( auto   & ele :  gain ) 
          { 
             ini += ele ; 
             mx = max ( mx , ini ) ; 
          } 
          return  mx ; 
      } 
 } ;