class   Solution   { 
 public : 
     string  largestOddNumber ( string num )   { 
         
         
         
          int  n = num . size ( ) - 1 ; 
         
         
          for ( int  j = n ; j >= 0 ; j -- ) 
          { 
              if ( ( num [ j ] - '0' ) % 2 != 0 ) 
              { 
                  return  num . substr ( 0 , j - 0 + 1 ) ; 
              } 
          } 
         
          return   "" ; 
      } 
 } ;